{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":timezone(Europe/Paris)",
    "github>pplancq/renovate-config:schedule"
  ],
  "labels": ["dependencies"],
  "assignees": ["pplancq"],
  "prConcurrentLimit": 10,
  "prHourlyLimit": 10,
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "packageRules": [
    {
      "packagePatterns": ["^@babel/"],
      "groupName": "babel packages"
    },
    {
      "packagePatterns": ["^@tanstack/react-query"],
      "groupName": "react-query packages"
    },
    {
      "packagePatterns": ["^@typescript-eslint/"],
      "groupName": "typescript-eslint packages"
    },
    {
      "updateTypes": ["major"],
      "labels": ["dependencies", "High"]
    }
  ]
}